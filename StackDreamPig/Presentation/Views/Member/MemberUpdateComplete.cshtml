
@{
    ViewData["Title"] = "EntryComplete";
}

<div class="container-fluid" style="padding-top: 100px;">
    <form class="form-horizontal" name="entry_form2" method="post">

        @{
            if (Model.isError)
            {
                <div class="form-group">
                    <div class="col-sm-offset-4 col-sm-10">
                        <button type="submit" asp-controller="Member" asp-action ="MemberUpdate" class="btn btn-success">編集画面に戻る</button>
                    </div>
                </div>
            }
            else
            {<h2>Complete</h2>
                <div class="form-group">
                    登録完了しました。
                </div>
                <div class="form-group">
                    <div class="col-sm-offset-4 col-sm-10">
                        <button type="submit" asp-controller="Home" asp-action ="IndexOrLogin" class="btn btn-success">機能一覧へ</button>
                    </div>
                </div>
            }

        }
    </form>
</div>